{
  "manifest_version": 2,

  "name": "Malicious_URL_Detector",
  "description": "This extension scores the URL, for a safer browsing ",
  "version": "1.1",

  "browser_action": {
    "default_popup": "home.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "32": "icon.png",
    "128": "icon.png"
  },
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
  "permissions": [
    "activeTab",
    "tabs",
    "tabs",
    "<all_urls>"
  ]
}
